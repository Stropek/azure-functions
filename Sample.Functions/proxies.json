{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "ProxySample": {
            "matchCondition": {
                "route": "proxysample",
                "methods": [ "POST", "OPTIONS" ]
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/HttpAdmin"
        },
        "Object": {
            "matchCondition": {
                "route": "object",
                "methods": [ "POST", "OPTIONS" ]
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/HttpObject"
        }
    }
}